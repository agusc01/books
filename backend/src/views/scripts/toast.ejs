<script>
    (()=>{
        let toasts = '<%- JSON.stringify(locals.toasts)%>';
        if(!toasts){ return; }
        toasts = JSON.parse(toasts);
        if (toasts && toasts?.length > 0) {
            for (let i = 0; i < toasts.length; i++) {
                toast(toasts[i]);
            }
        }
        '<%- locals.toasts = null %>'
    })();
</script>